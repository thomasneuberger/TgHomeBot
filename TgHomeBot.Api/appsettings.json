{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore.Mvc": "Warning",
        "Microsoft.AspNetCore.Hosting": "Warning",
        "Microsoft.AspNetCore.Routing": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "SmartHome": {
    "MonitoredDevices": []
  },
  "HomeAssistant": {
    "BaseUrl": "<set externally>",
    "Token": "<set externally>"
  },
  "Telegram": {
    "Token": "<set externally>",
    "AllowedUserNames": []
  },
  "FileStorage": {
    "Path": "<set externally>"
  }
}
